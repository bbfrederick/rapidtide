#!/bin/bash

rapidtide \
    --spatialfilt -1 \
    --nprocs -1 \
    --searchrange -5 20 \
    --simcalcrange 50 -1 \
    --outputlevel more \
    --graymattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_GRAY \
    --brainmask sub-RAPIDTIDETEST_brainmask.nii.gz \
    --whitemattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_WHITE \
    --csfmask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_CSF \
    --cleanrefined \
    --nofitfilt \
    --similaritymetric correlation \
    sub-RAPIDTIDETEST.nii.gz \
    ../dst/sub-RAPIDTIDETEST_multiproc_oldalloc_oldrefine
rapidtide \
    --spatialfilt -1 \
    --nprocs -1 \
    --searchrange -5 20 \
    --simcalcrange 50 -1 \
    --outputlevel more \
    --graymattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_GRAY \
    --brainmask sub-RAPIDTIDETEST_brainmask.nii.gz \
    --whitemattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_WHITE \
    --csfmask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_CSF \
    --cleanrefined \
    --nofitfilt \
    --newallocator \
    --similaritymetric correlation \
    sub-RAPIDTIDETEST.nii.gz \
    ../dst/sub-RAPIDTIDETEST_multiproc_newalloc_oldrefine
rapidtide \
    --spatialfilt -1 \
    --nprocs -1 \
    --searchrange -5 20 \
    --simcalcrange 50 -1 \
    --outputlevel more \
    --graymattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_GRAY \
    --brainmask sub-RAPIDTIDETEST_brainmask.nii.gz \
    --whitemattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_WHITE \
    --csfmask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_CSF \
    --cleanrefined \
    --nofitfilt \
    --newrefinedelay \
    --similaritymetric correlation \
    sub-RAPIDTIDETEST.nii.gz \
    ../dst/sub-RAPIDTIDETEST_multiproc_oldalloc_newrefine
rapidtide \
    --spatialfilt -1 \
    --nprocs -1 \
    --searchrange -5 20 \
    --simcalcrange 50 -1 \
    --outputlevel more \
    --graymattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_GRAY \
    --brainmask sub-RAPIDTIDETEST_brainmask.nii.gz \
    --whitemattermask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_WHITE \
    --csfmask sub-RAPIDTIDETEST_synthseg.nii.gz:SSEG_CSF \
    --cleanrefined \
    --nofitfilt \
    --newallocator \
    --newrefinedelay \
    --similaritymetric correlation \
    sub-RAPIDTIDETEST.nii.gz \
    ../dst/sub-RAPIDTIDETEST_multiproc_newalloc_newrefine
