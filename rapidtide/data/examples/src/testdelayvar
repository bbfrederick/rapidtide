#!/bin/bash

delayvar \
    sub-RAPIDTIDETEST.nii.gz \
    ../dst/sub-RAPIDTIDETEST \
    --alternateoutput ../dst/delayvartest \
    --nprocs -1 \
    --windowsize 20 \
    --systemicfittype pca \
    --trainrange -2.0 4.0 \
    --pcacomponents 0.8 \
    --outputlevel max \
    --focaldebug \
    --windelayoffsetspatialfilt 3
