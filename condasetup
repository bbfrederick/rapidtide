#!/bin/bash

conda create --name mic python==3.12
conda activate mic
mamba install \
    numpy \
    scipy \
    matplotlib>=3.3.0 \
    pandas \
    pyarrow \
    statsmodels \
    scikit-learn \
    scikit-image \
    nibabel \
    nilearn \
    pyfftw \
    pywavelets \
    pyqt \
    pyqt5-sip \
    pyqtgraph>=0.12.0 \
    tomlkit \
    tqdm

mamba install tensorflow keras
