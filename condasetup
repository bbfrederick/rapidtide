#!/bin/bash

conda create --name micfix python
conda init
conda activate micfix
mamba install \
    numpy \
    scipy \
    matplotlib \
    pandas \
    pyarrow \
    statsmodels \
    scikit-learn \
    scikit-image \
    nibabel \
    nilearn \
    pyfftw \
    pywavelets \
    pyqt6 \
    pyqt6-sip \
    pyqtgraph \
    tqdm \
    versioneer \
    requests

mamba install \
    tf-keras \
    tensorflow 
