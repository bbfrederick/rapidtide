#!/bin/bash

conda create --name rapidtide
conda activate rapidtide
conda install \
    nomkl \
    numpy \
    scipy \
    matplotlib \
    pandas \
    statsmodels \
    scikit-learn \
    nibabel \
    nilearn \
    pyfftw \
    numba \
    pyqt \
    pyqtgraph \
    tensorflow \
    keras \
    tqdm

