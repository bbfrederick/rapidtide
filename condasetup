#!/bin/bash

conda create --name rapidtide
conda activate rapidtide
conda install \
    numpy \
    scipy \
    matplotlib \
    pandas \
    statsmodels \
    scikit-learn \
    scikit-image \
    nibabel \
    nilearn \
    pyfftw \
    numba \
    pywavelets \
    pyqt \
    pyqt5-sip \
    "pyqtgraph<0.13" \
    tensorflow \
    keras \
    tqdm

